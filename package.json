{
  "name": "react-super-heroes",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.11.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5"
  },
  "scripts": {
    "lint": "eslint ./src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "danger": "DANGER_GITHUB_API_TOKEN=0fd68ae1cea37df814ee9ac68a2dce72ca559dc9 danger ci",
    "danger:pr": "DANGER_GITHUB_API_TOKEN=0fd68ae1cea37df814ee9ac68a2dce72ca559dc9 danger pr https://github.com/ardiadrianadri/super-heroe-react/pull/23d"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "danger": "^7.0.17",
    "eslint": "5.12.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "1.16.4"
  }
}
